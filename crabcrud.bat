echo URLs to download: %urls%

mkdir "%foldername%"/

setlocal EnableDelayedExpansion
set counter=0

for %%i in (%urls%) do (
    set /A counter+=1
    set "url_!counter!=%%~i"
)

for /L %%i in (1,1,%counter%) do (
    powershell -command "(New-Object System.Net.WebClient).DownloadFile('!url_%%i!', '%cd%\%foldername%\%%i.png')"
    if %errorlevel% EQU 0 (
        echo %%i.png downloaded successfully to %cd%\%foldername%
    ) else (
        echo failed to download %%i.png
    )
)

